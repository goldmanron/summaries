\end_inset

:
\begin_inset Formula 
\[
f_{n}\left(z\right)=\sum_{k=0}^{\infty}a_{n,k}z^{k}
\]

\end_inset

בנוסף לכל 
\begin_inset Formula $z$
\end_inset


\begin_inset Formula 
\[
\left|f_{n}\left(z\right)\right|\leq1
\]

\end_inset

אז לכל 
\begin_inset Formula $0<r<1$
\end_inset


\begin_inset Formula 
\[
\left|a_{n,k}\right|=\left|\frac{1}{2\pi i}\intop_{C_{r}}\frac{f_{n}\left(z\right)}{z^{k+1}}dz\right|\leq\frac{1}{2\pi}\cdot\frac{1\cdot2\pi r}{r^{k+1}}=\frac{1}{r^{k}}\Rightarrow\left|a_{n,k}\right|\leq1
\]

\end_inset


\begin_inset Formula $\left\{ a_{n,k}\right\} _{n,k\geq1}\subset D_{0}\left(1\right)$
\end_inset

 אז יש תת
\begin_inset Quotes erd
\end_inset

ס 
\begin_inset Formula $\left\{ n_{j}\right\} $
\end_inset

 כך שלכל 
\begin_inset Formula $k$
\end_inset

 הסדרה 
\begin_inset Formula $a_{n_{j},k}$
\end_inset

 מתכנסת כש-
\begin_inset Formula $j\rightarrow\infty$
\end_inset

.
\end_layout

\begin_layout Proof
לפיכך מועמדת לפונקציה הגבולית תהא 
\begin_inset Formula $f\left(z\right)=\sum_{k=0}^{\infty}a_{k}z^{k}$
\end_inset

,
 אשר מתכנסת בדיסק לפי קושי-הדמר.
\end_layout

\begin_layout Proof
צ
\begin_inset Quotes erd
\end_inset

ל 
\begin_inset Formula $f_{n_{j}}\rightarrow f$
\end_inset

 במ
\begin_inset Quotes erd
\end_inset

ש מקומית.
 אכן:
\begin_inset Formula 
\[
\left|f\left(z\right)-f_{n_{j}}\left(z\right)\right|=\left|\sum_{k=0}^{\infty}\left(a_{k}-a_{n_{j},k}\right)z^{k}\right|
\]

\end_inset

נבחר מספר שלם 
\begin_inset Formula $m$
\end_inset

 (אח
\begin_inset Quotes erd
\end_inset

כ):
\begin_inset Formula 
\[
\leq\sum_{k=0}^{m}\left|a_{k}-a_{n_{j},k}\right|+2\sum_{k=m+1}^{\infty}r^{k}=\sum_{k=0}^{m}\left|a_{k}-a_{n_{j},k}\right|+\frac{2\cdot r^{m+1}}{1-r}
\]

\end_inset

אז בהינתן 
\begin_inset Formula $\varepsilon>0$
\end_inset

,
 נבחר 
\begin_inset Formula $m$
\end_inset

 כך ש-
\begin_inset Formula 
\[
\frac{2r^{m+1}}{1-r}<\frac{\varepsilon}{2}
\]

\end_inset

ו-
\begin_inset Formula $j_{0}$
\end_inset

 יהיה האינדקס עבורו 
\begin_inset Formula $\left|a_{k}-a_{n_{j},k}\right|<\frac{\varepsilon}{2\left(m+1\right)}$
\end_inset

 לכל 
\begin_inset Formula $j>j_{0}$
\end_inset

,
 
\begin_inset Formula $k=0,\ldots,m$
\end_inset

 נקבל:
\end_layout

\begin_layout Proof
לכל 
\begin_inset Formula $\varepsilon>0$
\end_inset

,
 יש 
\begin_inset Formula $j_{0}$
\end_inset

 כך שאם 
\begin_inset Formula $j\geq j_{0}$
\end_inset


\begin_inset Formula 
\[
\left|f\left(z\right)-f_{n_{j}}\left(z\right)\right|<\frac{\varepsilon}{2}+\frac{\varepsilon}{2}=\varepsilon
\]

\end_inset


\end_layout

