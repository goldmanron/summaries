 & \text{\textbf{function}}\;\text{SieveOfEratosthenesAlgorithm}\left(n\right)\\
 & \qquad A\leftarrow\left[\underset{2}{\text{true}},\underset{3}{\text{true}},\ldots,\underset{n}{\text{true}}\right]\\
 & \qquad\text{\textbf{for}}\;i\leftarrow2\ldots n\\
 & \qquad\qquad\text{\textbf{if}}\;A\left[i\right]=\text{true}\\
 & \qquad\qquad\qquad j\leftarrow1\\
 & \qquad\qquad\qquad\text{\textbf{while}}\;ij\le n\\
 & \qquad\qquad\qquad\qquad A\left[ij\right]=\text{false}\\
 & \qquad\qquad\qquad\qquad j\leftarrow j+1\\
 & \qquad\text{\textbf{return}}\;A
\end{align*}

