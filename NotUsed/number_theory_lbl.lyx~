\begin_layout Standard

\series bold
חזקה מושלמת
\series default
: 
\begin_inset Formula $n\in\bbn$
\end_inset

 עבורו קיימים 
\begin_inset Formula $a\in\bbn_{+}$
\end_inset

 וכן 
\begin_inset Formula $b\in\bbn_{>1}$
\end_inset

 המקיימים 
\begin_inset Formula $n=a^{b}$
\end_inset

.
\end_layout

\begin_layout Standard

\series bold
מקדם בינומי
\series default
: יהיו 
\begin_inset Formula $k,n\in\bbn$
\end_inset

 באשר 
\begin_inset Formula $k\le n$
\end_inset

 אזי 
\begin_inset Formula ${n \choose k}=\frac{n!}{k!\left(n-k\right)!}$
\end_inset

.
\end_layout

\begin_layout Standard

\series bold
סימון
\series default
: יהיו 
\begin_inset Formula $k,n\in\bbn$
\end_inset

 אזי 
\begin_inset Formula $S_{n}^{\left(k\right)}=\sum_{i=1}^{n}i^{k}$
\end_inset

.
\end_layout

\begin_layout Standard

\series bold
טענה
\series default
: 
\begin_inset Formula $S_{n}^{\left(0\right)}=n$
\end_inset

, 
\begin_inset Formula $S_{n}^{\left(1\right)}=\frac{n\left(n+1\right)}{2}$
\end_inset

, 
\begin_inset Formula $S_{n}^{\left(2\right)}=\frac{n\left(n+1\right)\left(2n+1\right)}{6}$
\end_inset

.
\end_layout

\begin_layout Standard

\series bold
הבינום של ניוטון
